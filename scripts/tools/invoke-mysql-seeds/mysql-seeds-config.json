{
  "version": "1.0.0",
  "description": "Configuracion para Invoke-MySqlSeeds: migraciones EF y seeds Admin (GesFer.Admin.Back).",
  "efProject": "src/GesFer.Admin.Back.Infrastructure/GesFer.Admin.Back.Infrastructure.csproj",
  "startupProject": "src/GesFer.Admin.Back.Api/GesFer.Admin.Back.Api.csproj",
  "seedsPath": "src/GesFer.Admin.Back.Infrastructure/Data/Seeds",
  "connectionEnv": "ConnectionStrings__DefaultConnection",
  "runMigrations": true,
  "runSeeds": true,
  "mysqlContainerName": "gesfer_db",
  "healthCheck": {
    "mysqlPingMaxAttempts": 15,
    "mysqlPingRetrySeconds": 2
  }
}
